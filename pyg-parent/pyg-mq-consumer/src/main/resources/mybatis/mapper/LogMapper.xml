<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.pyg.mq.consumer.mapper.LogMapper" >

  <resultMap id="BaseResultMap" type="com.pyg.pojo.User" >
    <id column="log_uuid" property="logUuid" jdbcType="VARCHAR" />

  </resultMap>


  <insert id="insertLog" parameterType="LogInfo" >
    insert into tb_log (log_uuid)
    values (#{logUuid,jdbcType=VARCHAR})
  </insert>





</mapper>